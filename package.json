{
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-array-func": "^4.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^6.5.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^7.18.0"
  },
  "description": "Safety-oriented, fp-first configuration of ESLint.",
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/plugin-transform-typescript": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^8.57.1",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.75",
    "babel-jest": "^29.7.0",
    "commitizen": "^4.3.1",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.12.0 <=20"
  },
  "files": [
    "index.js",
    "rules",
    "README.md"
  ],
  "license": "UNLICENSED",
  "lint-staged": {
    "*.{js,json,md,ts,yml,yaml}": [
      "prettier --write"
    ]
  },
  "main": "./index.js",
  "name": "@ridedott/eslint-config",
  "peerDependencies": {
    "eslint": "^8.57.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ridedott/eslint-config.git"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "cz": "git-cz",
    "cz:retry": "git-cz --retry",
    "format": "prettier --check '**/*.{js,json,md,ts,yml,yaml}'",
    "format:fix": "prettier --write '**/*.{js,json,md,ts,yml,yaml}'",
    "test": "export NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\"; jest",
    "test:watch": "jest --watchAll"
  },
  "version": "3.0.1"
}
