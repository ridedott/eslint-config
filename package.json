{
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ridedott/eslint-plugin": "^1.7.149",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-array-func": "^4.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unicorn": "^49.0.0",
    "typescript": "^5.3.2"
  },
  "description": "Safety-oriented, fp-first configuration of ESLint.",
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/plugin-transform-typescript": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/eslint": "^8.44.8",
    "@types/jest": "^29.5.10",
    "@types/node": "^16.18.66",
    "babel-jest": "^29.7.0",
    "commitizen": "^4.3.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=18.12.0 <=20"
  },
  "files": [
    "index.js",
    "rules",
    "README.md"
  ],
  "license": "UNLICENSED",
  "lint-staged": {
    "*.{js,json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "main": "./index.js",
  "name": "@ridedott/eslint-config",
  "peerDependencies": {
    "eslint": "^8.52.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ridedott/eslint-config.git"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "cz": "git-cz",
    "cz:retry": "git-cz --retry",
    "format": "prettier --check '**/*.{js,json,md,ts,yml,yaml}'",
    "format:fix": "prettier --write '**/*.{js,json,md,ts,yml,yaml}'",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "version": "2.27.24"
}
